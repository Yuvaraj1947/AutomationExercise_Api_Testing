		{
			"name": "POST To Create/Register User Account",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "09ced35b-228e-4901-bf6d-52a9f64bb6e0",
						"exec": [
							"// 1. Validate response status code is 200 or 201",
							"pm.test(\"Status code is 200 or 201\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
							"});",
							"",
							"// 2. Check for success message or user ID in response body",
							"pm.test(\"Response contains success message or user ID\", function () {",
							"    let jsonData;",
							"    try {",
							"        jsonData = pm.response.json();",
							"    } catch (e) {",
							"        pm.expect.fail(\"Response is not valid JSON\");",
							"    }",
							"    if (pm.response.code === 200 || pm.response.code === 201) {",
							"        pm.expect(",
							"            jsonData.hasOwnProperty(\"message\") && /success/i.test(jsonData.message) ||",
							"            jsonData.hasOwnProperty(\"userId\") ||",
							"            jsonData.hasOwnProperty(\"id\")",
							"        ).to.be.true;",
							"    }",
							"});",
							"",
							"// 3. Log error message if response indicates error (e.g., 400)",
							"if (pm.response.code >= 400) {",
							"    let jsonData;",
							"    try {",
							"        jsonData = pm.response.json();",
							"        if (jsonData.message) {",
							"            console.log(\"API Error Message:\", jsonData.message);",
							"        } else {",
							"            console.log(\"API Error Response:\", JSON.stringify(jsonData));",
							"        }",
							"    } catch (e) {",
							"        console.log(\"Non-JSON error response:\", pm.response.text());",
							"    }",
							"}",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
